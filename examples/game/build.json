{
	"source_dir": "./",
	"destination_dir": "build/examples/game/",
	"html": "index.html",
	"sources": [
		{
			"bundle": [
				"src/meta.js",
				"src/util.js",
				"src/load_state.js",
				"src/entity.js",
				"src/world.js",
				"src/game_state.js",
				"src/assets.js",
				"src/init.js"
			],
			"output_file": "src/game.min.js",
			"compile_options": "--mangle --compress --enclose",
			"preprocess": true,
			"variables": {
				"debug": [
					"DEBUG=true"
				]
			},
			"header": [
				"/* YABS.js game example v%version%",
				" * (c) $YEAR$ %author% */"
			]
		}
	],
	"files": [
		"css/*.css",
		"data/*",
		"lib/*.js"
	]
}
